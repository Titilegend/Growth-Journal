<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.InnerGrowthApp" parent="Theme.Material3.DayNight.NoActionBar">
         <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_theme</item>
        <item name="colorPrimaryVariant">@color/purple_theme</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#F0F6F6</item>
        <item name="colorSecondaryVariant">@color/purple_theme</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

    </style>

    <style name="Theme.InnerGrowthApp2" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/purple_theme</item>
        <item name="colorPrimaryVariant">@color/purple_theme</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#F0F6F6</item>
        <item name="colorSecondaryVariant">@color/purple_theme</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>
</resources>